{
  "name": "Z-Wave JS 2 MQTT",
  "version": "1.4-6.5-2sec",
  "slug": "zwavejs2mqtt",
  "description": "Fully configurable Zwave to MQTT Gateway and Control Panel.",
  "url": "https://zwave-js.github.io/zwavejs2mqtt",
  "startup": "system",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "map": ["config:rw"],
  "boot": "auto",
  "webui": "http://[HOST]:[PORT:8091]",
  "ingress": true,
  "ingress_port": 8091,
  "panel_icon": "mdi:z-wave",
  "options": {
    "serial_port": "/dev/ttyACM0",
    "network_key": ""
  },
  "schema": {
    "serial_port": "str",
    "network_key": "str"
  },
  "ports": {
    "3000/tcp": 3000,
    "8091/tcp": 8091
  },
  "auto_uart": true
}
